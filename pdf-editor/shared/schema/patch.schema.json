{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Patch operations",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["op", "target"],
    "properties": {
      "op": {"type": "string"},
      "target": {
        "type": "object",
        "required": ["page", "id"],
        "properties": {
          "page": {"type": "integer", "minimum": 0},
          "id": {"type": "string"}
        }
      }
    },
    "additionalProperties": true
  }
}
