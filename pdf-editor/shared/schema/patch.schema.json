{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Patch",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["op", "target"],
    "properties": {
      "op": { "type": "string" },
      "target": {
        "type": "object",
        "required": ["page", "id"],
        "properties": {
          "page": { "type": "integer" },
          "id": { "type": "string" }
        }
      }
    }
  }
}
